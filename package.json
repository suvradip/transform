{
   "name": "convo",
   "proxy": "http://localhost:8888",
   "version": "0.1.0",
   "private": true,
   "dependencies": {
      "@skidding/react-codemirror": "^1.0.2",
      "axios": "^0.19.0",
      "body-parser": "^1.19.0",
      "codemirror": "^5.50.2",
      "cors": "^2.8.5",
      "debug": "^4.1.1",
      "dotenv": "^8.2.0",
      "express": "^4.17.1",
      "express-cache-controller": "^1.1.0",
      "morgan": "^1.9.1",
      "node-sass": "^4.13.0",
      "prop-types": "^15.7.2",
      "react": "^16.12.0",
      "react-copy-to-clipboard": "^5.0.2",
      "react-dom": "^16.12.0",
      "react-redux": "^7.1.3",
      "react-scripts": "3.3.0",
      "redux": "^4.0.5"
   },
   "devDependencies": {
      "eslint-config-airbnb": "^18.0.1",
      "eslint-config-prettier": "^6.9.0",
      "eslint-plugin-prettier": "^3.1.2",
      "prettier": "^1.19.1"
   },
   "scripts": {
      "start": "react-scripts start",
      "build": "react-scripts build",
      "test": "react-scripts test --env=jsdom",
      "eject": "react-scripts eject",
      "start:server": "node server/index.js",
      "start:nodemon": "nodemon src/index.js",
      "debug": "nodemon --inspect-brk src/index.js"
   },
   "browserslist": {
      "production": [
         ">0.2%",
         "not dead",
         "not op_mini all"
      ],
      "development": [
         "last 1 chrome version",
         "last 1 firefox version",
         "last 1 safari version"
      ]
   },
   "eslintConfig": {
      "extends": [
         "react-app",
         "airbnb",
         "prettier",
         "prettier/react"
      ],
      "plugins": [
         "prettier"
      ],
      "globals": {
         "Highcharts": true,
         "FusionCharts": true
      },
      "env": {
         "browser": true
      },
      "rules": {
         "prettier/prettier": [
            "error"
         ],
         "max-len": [
            "error",
            {
               "code": 100
            }
         ],
         "react/jsx-filename-extension": [
            1,
            {
               "extensions": [
                  ".js",
                  ".jsx"
               ]
            }
         ],
         "react/prop-types": [
            "warn"
         ],
         "no-return-assign": [
            "off"
         ],
         "no-unused-vars": [
            "error"
         ],
         "react/forbid-prop-types": [
            "off"
         ]
      }
   }
}
