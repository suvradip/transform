{
   "editor.formatOnSave": true,
   "[javascript]": {
      "editor.formatOnSave": false
   },
   "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
   },
   "eslint.alwaysShowStatus": true,

   "cSpell.words": [
      "codemirror",
      "foldcode",
      "foldgutter",
      "linenumbers",
      "matchbrackets"
   ],
   "eslint.validate": ["javascript", "javascriptreact"]
}
